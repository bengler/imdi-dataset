{
  "name": "@bengler/imdi-dataset",
  "version": "1.0.11",
  "description": "",
  "main": "index.js",
  "scripts": {
    "import": "DEBUG=imdi-dataset* babel-node --experimental import.js",
    "import-historical": "DEBUG=imdi-dataset* babel-node --experimental import-historical.js",
    "make-tree": "DEBUG=imdi-dataset* babel-node --experimental make-tree.js",
    "all": "npm run import && npm run import-historical && npm run make-tree",
    "test": "mocha -r babel/register test",
    "prepublish": "rm -Rf .prepublish && mkdir .prepublish && cp -r *.js lib .prepublish/ && babel *.js --out-dir . && babel lib --out-dir lib",
    "postpublish": "cp -r .prepublish/* . && rm -Rf .prepublish && git status"
  },
  "author": "Bengler AS",
  "license": "ISC",
  "dependencies": {
    "babel": "^4.7.16",
    "bluebird": "^2.9.14",
    "csv-parse": "0.1.0",
    "debug": "^2.1.3",
    "dotty": "0.0.2",
    "flat": "^1.5.1",
    "lodash.pick": "^3.0.0",
    "rx": "^2.4.3",
    "split": "^0.3.3"
  },
  "devDependencies": {
    "chai": "^2.1.2",
    "mocha": "^2.2.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "experimental": true
        }
      ]
    ]
  }
}
